<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<nifty>
  <useStyles filename="nifty-default-styles.xml"/>
  <useControls filename="nifty-default-controls.xml"/>
  
<screen id="mainScreen" controller="drossy.stars.core.client.MainMenuState">
    <layer childLayout="absolute" id="mainLayer">
      <panel childLayout="absolute" height="460px" id="buttonPanel" style="nifty-panel-simple" width="319px" x="226" y="47">
        <control childLayout="center" height="50px" id="debugSceneBtn" label="Debug Scene" name="button" width="270px" x="20" y="15">
            <interact onClick="loadDebugScene()" />
        </control>
        <control childLayout="center" height="50px" id="modelEditorBtn" label="Model editor" name="button" width="270px" x="20" y="70">
            <interact onClick="loadModelEditor()" />
        </control>
        <control childLayout="center" height="50px" id="exitButton" label="Exit" name="button" width="270px" x="20" y="390">
            <interact onClick="quit()" />
        </control>
      </panel>
    </layer>
</screen>
</nifty>